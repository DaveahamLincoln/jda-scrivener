<h1>Listing wfm_alerts</h1>

<table>
  <thead>
    <tr>
      <th>From</th>
      <th>Subject</th>
      <th>Pool</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wfm_alerts.each do |wfm_alert| %>
      <tr>
        <td><%= wfm_alert.source %></td>
        <td><%= wfm_alert.subject %></td>
        <td><%= link_to 'Show', wfm_alert %></td>
        <td><%= link_to 'Edit', edit_wfm_alert_path(wfm_alert) %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
